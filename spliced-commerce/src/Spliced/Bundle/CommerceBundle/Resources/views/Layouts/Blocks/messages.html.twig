{% for flash in ['warning','error','info','success'] %}
	{% if app.session.flashBag.has(flash) %}
		{% for message in app.session.flashBag.get(flash) %}
		<div class="main-alert alert alert-{{ flash }}">
			<button type="button" class="close" data-dismiss="alert">&times;</button>
			{{ message|raw }}
		</div>
		{% endfor %}
	{% endif %}
{% endfor %}
