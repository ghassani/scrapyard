			<tr class="project_invoice_{{  invoice.id }}">
				<td>{{ invoice.id }}</td>
				<td>${{ invoice.totalAmount }}</td>
				<td>{{ invoice.lineItems|length }}</td>
				<td>{{ invoice.startDate|length>0 ? invoice.startDate|date('m/d/y') : '' }}</td>
				<td>{{ invoice.completionDate|length>0 ? invoice.completionDate|date('m/d/y') : '' }}</td>
				<td></td>
				<td>
					<a href="{{ path('dashboard', { }) }}" class="ajax_link_confirm btn btn-primary btn-small"><i class="icon-pencil icon-white"></i> Edit</a>
					<a href="{{ path('dashboard', { }) }}" class="ajax_link_confirm btn btn-inverse btn-small"><i class="icon-print icon-white"></i> PDF</a>
					<a href="{{ path('project_invoice_delete', { 'project' : invoice.project.id, 'quote' : invoice.id }) }}" class="ajax_link_confirm btn btn-danger btn-small" data-message="Are you sure you want to delete this invoice?"><i class="icon-remove-sign icon-white"></i> Delete</a>
				</td>
			</tr>
		